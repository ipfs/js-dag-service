{
  "name": "@textile/ipfs-lite",
  "version": "0.1.5",
  "description": "A lightweight minimal IPFS peer for typescript/javascript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": {
    "./setup/node": "./setup/browser"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "polendina --cleanup tests/**/*.p.test.ts --webpack-config webpack.test.js",
    "test:node": "TS_NODE_FILES=true mocha -r ts-node/register -r source-map-support/register --recursive tests/**/*.test.ts --timeout 8000 --exit",
    "lint": "eslint --fix --ext .mjs,.js,.jsx,.ts,.tsx src",
    "docs:build": "jsdoc -c jsdoc.json",
    "docs:serve": "npm run docs:build && http-server docs/",
    "docs": "npm run docs:build",
    "browser:build": "NODE_ENV=production webpack --mode production",
    "browser:serve": "npm run browser:build && http-server dist/browser --gzip",
    "browser": "npm run browser:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textileio/js-ipfs-lite.git"
  },
  "keywords": [
    "ipfs",
    "lite",
    "dag",
    "typescript",
    "browser",
    "node"
  ],
  "author": "Carson Farmer <carson@textile.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/textileio/js-ipfs-lite/issues"
  },
  "homepage": "https://github.com/textileio/js-ipfs-lite#readme",
  "devDependencies": {
    "@ipld/codec-interface": "^1.0.7",
    "@types/chai": "^4.2.7",
    "@types/datastore-core": "^0.7.0",
    "@types/interface-datastore": "^0.8.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "better-docs": "^1.4.7",
    "cids": "^0.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "html-webpack-plugin": "^4.0.3",
    "http-server": "^0.12.0",
    "ipfs-block-service": "^0.17.1",
    "ipld": "^0.25.0",
    "ipld-block": "^0.9.0",
    "ipld-dag-pb": "^0.18.1",
    "jsdoc": "^3.6.3",
    "libp2p-pubsub": "^0.4.3",
    "multihashing-async": "^0.8.0",
    "peer-id": "^0.12.2",
    "peer-info": "^0.17.1",
    "polendina": "^1.0.0",
    "prettier": "^1.18.2",
    "resolve-ts-aliases": "^1.0.0",
    "streaming-iterables": "^4.1.1",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "mocha": "^7.0.0",
    "ts-node": "^8.6.0"
  },
  "dependencies": {
    "@ipld/block": "^4.0.2",
    "bignumber.js": "^9.0.0",
    "ipfs-bitswap": "^0.26.0",
    "ipfs-unixfs-exporter": "^2.0.1",
    "ipfs-unixfs-importer": "^1.0.3",
    "libp2p": "^0.26.2",
    "libp2p-bootstrap": "^0.9.7",
    "libp2p-gossipsub": "0.0.5",
    "libp2p-kad-dht": "^0.16.0",
    "libp2p-mdns": "^0.12.3",
    "libp2p-mplex": "^0.9.5",
    "libp2p-secio": "^0.12.4",
    "libp2p-tcp": "^0.13.2",
    "libp2p-webrtc-star": "^0.17.7",
    "libp2p-websocket-star-multi": "^0.4.4",
    "libp2p-websockets": "^0.12.4",
    "merge-options": "^2.0.0",
    "pull-mplex": "^0.1.2",
    "tslib": "^1.10.0"
  }
}
